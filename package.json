{
  "name": "mantine-next-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn codegen:watch\"",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "export": "next build && next export",
    "lint:fix": "yarn lint --fix; yarn format",
    "lint": "eslint --ignore-path .eslintignore \"./**/*.+(ts|js|tsx)\"",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "npm run prettier:check && npm run lint && npm run typecheck && npm run jest",
    "storybook": "start-storybook -p 7001",
    "storybook:build": "build-storybook",
    "format": "prettier --ignore-path .eslintignore \"./**/*.+(ts|js|tsx)\" --write",
    "codegen": "graphql-codegen",
    "codegen:watch": "yarn codegen --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
