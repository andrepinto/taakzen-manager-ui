    "react": "18.2.0",
    "react-dom": "18.2.0",
    "urql": "^3.0.3"
  },
  "resolutions": { "@types/react": "18.0.21", "@types/react-dom": "18.0.10" },
  "devDependencies": {
    "@babel/core": "^7.17.8",
